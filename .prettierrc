{
    "printWidth": 100,
    "tabWidth": 2,
    "semi": true,
    "useTabs": false,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "auto",
    "overrides": [
        {
            "files": ["*.scss"],
            "options": {
                "singleQuote": false
            }
        }
    ]
}
